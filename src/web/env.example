# Google OAuth (for cloud mode)
# Get from: Google Cloud Console > APIs & Services > Credentials
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth (for cloud mode)
# Get from: Facebook Developers Console > Your App > Settings > Basic
# Valid OAuth Redirect URIs: https://cloud.brewos.io/, http://localhost:5173/
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Cloud WebSocket URL (for cloud mode)
VITE_CLOUD_WS_URL=wss://cloud.brewos.io/ws

# Environment (staging or production)
# Used to distinguish between staging and production deployments
VITE_ENVIRONMENT=production
